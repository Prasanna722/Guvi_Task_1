public class Main {
    public static void main(String[] args) {
        for (int i = 0; i < 5; i++) {
            for (int j = 5; j > 5 - i - 1; j--) {
                System.out.print(j);
            }
            for (int k = 0; k < 4 - i; k++) {
                System.out.print(5 - i);
            }
            System.out.println();
        }
    }
}
